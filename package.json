{
  "private": true,
  "name": "preact-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build:android": "preact build --dest build/android --preload",
    "build:web": "preact build --dest build/web --preload",
    "build:ios": "preact build --dest build/ios --preload",
    "serve": "preact build && sirv build --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "jest",
    "build-all": "preact build --dest build/android --preload && preact build --dest build/ios --preload && preact build --dest build/web --preload",
    "web-redirection": "node jobs/web-redirection",
    "pwa-config": "node jobs/pwa-config.js"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.6.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-preact": "^1.1.4",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^4.0.3",
    "lint-staged": "^10.5.4",
    "node-sass": "^6.0.1",
    "per-env": "^1.0.2",
    "preact-cli": "^3.2.2",
    "preact-render-spy": "^1.2.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^2.0.2",
    "sass-loader": "^10.2.0",
    "sirv-cli": "^0.4.6",
    "webpack": "^4.46.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.2.0",
    "@wojtekmaj/react-daterange-picker": "^3.4.0",
    "axios": "^0.21.1",
    "clipboard-copy": "^4.0.1",
    "history": "^4.10.1",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "preact-router": "^3.2.1",
    "react-google-recaptcha": "^2.1.0",
    "react-responsive-carousel": "^3.2.22",
    "unistore": "^3.5.2"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix src"
    ]
  }
}
